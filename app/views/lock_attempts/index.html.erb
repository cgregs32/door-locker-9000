<p id="notice"><%= notice %></p>

<h1>Lock Attempts</h1>

<table>
  <thead>
    <tr>
      <th>Attempted</th>
      <th>Errors</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lock_attempts.each do |lock_attempt| %>
      <tr>
        <td><%= lock_attempt.attempted %></td>
        <td><%= lock_attempt.errors %></td>
        <td><%= link_to 'Show', lock_attempt %></td>
        <td><%= link_to 'Edit', edit_lock_attempt_path(lock_attempt) %></td>
        <td><%= link_to 'Destroy', lock_attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lock Attempt', new_lock_attempt_path %>
